{
  "plugins": ["@typescript-eslint/eslint-plugin"],
  "extends": [
    "next/core-web-vitals",
    "prettier",
    "plugin:prettier/recommended",
    "prettier",
    "plugin:@typescript-eslint/eslint-recommended",
    "plugin:react-hooks/recommended"
  ],
  "env": {
    "jest": true,
    "browser": true
  },
  "globals": {
    "__DEV__": true
  },
  "parserOptions": {
    "ecmaVersion": 2016,
    "ecmaFeatures": {
      "legacyDecorators": true,
      "jsx": true
    }
  },
  "settings": {
    "react": {
      "version": "18"
    }
  }
}
